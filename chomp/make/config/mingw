# Settings for the MinGW port of the GNU C++ compiler in Windows.

SYSLIB  = -lwsock32
SYSLIBG = -lwsock32
SYSINCL =
CXX = g++
COMPILE = $(CXX) -mwindows -O2 -ansi -pedantic -Wall
LINK    = $(CXX) -s
LINKGUI = $(CXX) -mwindows -mconsole -s
MAKELIB = ar cru
IDXLIB  = ranlib
REMOVE  = rm -f
MKDIR   = mkdir
EXE     = .exe
LIBEXT  = .a
OBJEXT  = .o

